{
  "manifest_version": 2,
  "name": "Copium",
  "version": "1.0.1.3",
  "description": "Simple save your twitch pastes and paste them LUL.",
  "icons": {
    "24": "icons/favicon.ico",
    "48": "icons/favicon.ico",
    "96": "icons/favicon.ico"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.twitch.tv/*"
      ],
      "js": [
        "dist/css.js",
        "dist/html.js",
        "copium.js"
      ]
    }
  ]
}